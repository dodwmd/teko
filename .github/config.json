{
  "repository": {
    "name": "teko",
    "description": "Teko (Task Engine KO) - AI-powered development automation system",
    "homepage": "https://github.com/dodwmd/teko",
    "topics": [
      "ai",
      "automation",
      "laravel",
      "python",
      "langchain",
      "development-tools"
    ],
    "private": false,
    "has_issues": true,
    "has_projects": true,
    "has_wiki": true,
    "has_downloads": true,
    "default_branch": "master",
    "allow_squash_merge": true,
    "allow_merge_commit": true,
    "allow_rebase_merge": true,
    "allow_auto_merge": false,
    "delete_branch_on_merge": true
  },
  "branch_protection_rules": [
    {
      "branch": "master",
      "required_status_checks": {
        "strict": true,
        "contexts": [
          "php-tests",
          "python-tests",
          "security-scan"
        ]
      },
      "enforce_admins": false,
      "required_pull_request_reviews": {
        "dismiss_stale_reviews": true,
        "require_code_owner_reviews": true,
        "required_approving_review_count": 1
      },
      "restrictions": null,
      "require_signed_commits": false,
      "required_linear_history": true,
      "required_conversation_resolution": true
    },
    {
      "branch": "develop",
      "required_status_checks": {
        "strict": true,
        "contexts": [
          "php-tests",
          "python-tests"
        ]
      },
      "enforce_admins": false,
      "required_pull_request_reviews": {
        "dismiss_stale_reviews": true,
        "require_code_owner_reviews": false,
        "required_approving_review_count": 1
      },
      "restrictions": null,
      "require_signed_commits": false,
      "required_linear_history": false,
      "required_conversation_resolution": true
    }
  ],
  "environments": [
    {
      "name": "production",
      "wait_timer": 60,
      "reviewers": [],
      "deployment_branch_policy": {
        "protected_branches": true,
        "custom_branch_policies": false
      }
    },
    {
      "name": "staging",
      "wait_timer": 0,
      "reviewers": [],
      "deployment_branch_policy": {
        "protected_branches": true,
        "custom_branch_policies": true
      }
    }
  ],
  "team_access": [
    {
      "team": "developers",
      "permission": "push"
    },
    {
      "team": "admins",
      "permission": "admin"
    }
  ],
  "collaborators": [
    {
      "username": "dodwmd",
      "permission": "admin"
    }
  ]
}
